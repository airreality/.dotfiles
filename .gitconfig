[credential]
	helper = store
[fetch]
	prune = true
[rerere]
	enabled = true
[color]
	ui = auto
[user]
	name = airreality
	email = airunreality@gmail.com
[includeIf "gitdir:~/work/"]
	path = ~/work/.gitconfig
[push]
	autoSetupRemote = true
[rebase]
	updateRefs = true
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge --skip -- %f
	process = git-lfs filter-process --skip
